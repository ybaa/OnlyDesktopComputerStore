<Window x:Class="onlyDesktop2.LogIn"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:onlyDesktop2"
        mc:Ignorable="d"
        Title="LogIn" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="mailTextBox" HorizontalAlignment="Left" Height="23" Margin="99,84,0,0" TextWrapping="Wrap" Text="mail" VerticalAlignment="Top" Width="120" GotFocus="mailTextBox_GotFocus"/>
        <TextBox x:Name="passwordTextBox" HorizontalAlignment="Left" Height="23" Margin="99,138,0,0" TextWrapping="Wrap" Text="hasło" VerticalAlignment="Top" Width="120" GotFocus="passwordTextBox_GotFocus"/>
        <Label x:Name="label" Content="mail" HorizontalAlignment="Left" Margin="34,81,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="haslo" HorizontalAlignment="Left" Margin="34,134,0,0" VerticalAlignment="Top"/>
        <Button x:Name="logInButton" Content="Zaloguj" HorizontalAlignment="Left" Margin="99,198,0,0" VerticalAlignment="Top" Width="75" Click="logInButton_Click"/>

    </Grid>
</Window>
